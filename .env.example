# Predibase and Upstage API keys (used for LLM inference or job matching models)
PREDIBASE_API_KEY=your_predibase_api_key_here
UPSTAGE_API_KEY=your_upstage_api_key_here

# PostgreSQL TimescaleDB connection string
DATABASE_URL=postgres://<username>:<password>@<host>:<port>/<database>?sslmode=require

# AWS configuration for SageMaker inference
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
SAGEMAKER_ENDPOINT_NAME=your_sagemaker_endpoint_name

# Supabase credentials
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_service_role_key

# Google OAuth for NextAuth.js
GOOGLE_CLIENT_ID=your_google_oauth_client_id
GOOGLE_CLIENT_SECRET=your_google_oauth_client_secret

# NextAuth.js session and deployment configuration
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# Public Supabase keys (safe to expose to frontend)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
